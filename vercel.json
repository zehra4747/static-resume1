{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dist/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "dist/script.js",
      "use": "@vercel/static"
    },
    {
      "src": "dist/images/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
